#!/usr/bin/python3
import calculator_1
if __name__=='__main__':
    a = 10
    b = 5
calculator_1.add(a, b)
print("{:d} + {:d} = ".format(a, b, calculator_1.add(a, b)))

calculator_1.sub(a, b)
print("{:d} - {:d} = {:d}".format(a, b, calculator_1.sub(a, b)))

calculator_1.mul
print("{:d} * {:} = {:d}".format(a, b, calculator_1.mul(a, b)))

calculator_1.div
print("{:d} / {:} = {:d}".format(a, b, calculator_1.div(a, b)))
